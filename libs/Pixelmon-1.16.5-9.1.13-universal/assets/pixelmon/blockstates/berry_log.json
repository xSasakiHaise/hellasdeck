{
  "multipart": [
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "pixelmon:block/berry_tree/log"
      }
    },
    {
      "when": {
        "north": "low"
      },
      "apply": {
        "model": "pixelmon:block/berry_tree/log_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "low"
      },
      "apply": {
        "model": "pixelmon:block/berry_tree/log_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "low"
      },
      "apply": {
        "model": "pixelmon:block/berry_tree/log_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "low"
      },
      "apply": {
        "model": "pixelmon:block/berry_tree/log_side",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "tall"
      },
      "apply": {
        "model": "pixelmon:block/berry_tree/log_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "tall"
      },
      "apply": {
        "model": "pixelmon:block/berry_tree/log_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "tall"
      },
      "apply": {
        "model": "pixelmon:block/berry_tree/log_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "tall"
      },
      "apply": {
        "model": "pixelmon:block/berry_tree/log_side",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}
