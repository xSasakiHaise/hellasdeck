{
  "multipart": [
    {
      "when": {
        "age": "0"
      },
      "apply": [
        {
          "model": "pixelmon:block/berry_tree/wepear/leaves"
        }
      ]
    },
    {
      "when": {
        "age": "1"
      },
      "apply": [
        {
          "model": "pixelmon:block/berry_tree/wepear/bloom"
        }
      ]
    },
    {
      "when": {
        "age": "2"
      },
      "apply": [
        {
          "model": "pixelmon:block/berry_tree/wepear/berry"
        }
      ]
    }
  ]
}